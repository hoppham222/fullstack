<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>get crud</title>
</head>
<body>
    <table>
        <thead>
            <tr>
                <td>email</td>
                <td>firstname</td>
                <td>lastname</td>
                <td>address</td>
                <td>actions</td>
            </tr>
        </thead>
        <tbody><% for(let i=0; i < data.length; i++) {%>
            <tr>
                <td><%= data[i].email %></td>
                <td><%= data[i].firstName %></td>
                <td><%= data[i].lastName %></td>
                <td><%= data[i].address %></td>
                <td>
                    <a href="/edit-crud?id=<%= data[i].id %>" type="button">Edit</a>
                    <a href="/delete-crud?id=<%= data[i].id %>" type="button">delete</a>
                </td>
            </tr>
            <% } %>
        </tbody>
    </table>
</body>
</html>